public class TechnicalEmployee extends Employee {
    public int checkIns;

    public TechnicalEmployee(String name) {
        // Has a default base salary of 75000
        super(name, 75000);
        checkIns = 0;
    }

    public String employeeStatus() {
        //Should return a String representation of this TechnicalEmployee that includes their ID,
        // name and how many successful check ins they have had. Example: "1 Kasey has 10 successful check ins"
        return toString()+" has "+checkIns+" successful check ins";
    }
}
